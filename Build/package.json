{
  "name": "sf_dalleimages",
  "description": "Bootstrap 5, Webpack, PostCSS, Stylelint and Eslint.",
  "main": "index.js",
  "private": true,
  "scripts": {
    "build": "webpack --config webpack.prod.js --mode production",
    "dev": "webpack --config webpack.dev.js --mode development",
    "watch": "webpack --config webpack.dev.js --mode development --watch",
    "lint:eslint": "eslint Assets/**/*.js",
    "lint:eslint.fix": "eslint --fix Assets/**/*.js",
    "lint:eslint.report.junit": "eslint -f junit -o reports/eslintreport.junit.xml Assets/**/*.js",
    "lint:stylelint.report.json": "stylelint --config stylelint.config.js -f json -o reports/stylelintreport.json Assets/Scss/**/*.scss",
    "lint:stylelint.report.junit": "stylelint --config stylelint.config.js --custom-formatter 'node_modules/stylelint-junit-formatter' -o reports/stylelintreport.junit.xml Assets/Scss/**/*.scss",
    "lint:stylelint.report.verbose": "stylelint --config stylelint.config.js -f verbose -o reports/stylelintreport.verbose.txt Assets/Scss/**/*.scss"
  },
  "author": "Andreas Kaufhold",
  "license": "ISC",
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.2",
    "vanilla-lazyload": "^17.8.5"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "braces": "3.0.3",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.54.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-webpack-plugin": "^4.0.1",
    "fast-glob": "^3.3.2",
    "mini-css-extract-plugin": "^2.7.6",
    "node-libcurl": "^4.0.0",
    "path": "^0.12.7",
    "pluralize": "^8.0.0",
    "postcss": "^8.4.31",
    "postcss-inline-svg": "^6.0.0",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.3.0",
    "postcss-pxtorem": "^6.0.0",
    "postcss-svgo": "^6.0.0",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "stylelint": "^15.11.0",
    "stylelint-config-recommended": "^13.0.0",
    "stylelint-config-recommended-scss": "^13.1.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-junit-formatter": "^0.2.2",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^5.3.1",
    "stylelint-webpack-plugin": "^4.1.1",
    "ua-parser-js": "^1.0.37",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-shell-plugin-next": "^2.3.1"
  },
  "packageManager": "yarn@4.3.1"
}
